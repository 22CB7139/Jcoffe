<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>Behinder</filter-name>
    <filter-class>com.sorry.bug.BehinderFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Behinder</filter-name>
    <url-pattern>/behinder</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>FilterInject</filter-name>
    <filter-class>com.sorry.bug.InjectFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterInject</filter-name>
    <url-pattern>/injectfilter</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>ServletXml</servlet-name>
    <servlet-class>com.sorry.bug.CustomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletXml</servlet-name>
    <url-pattern>/customservlet</url-pattern>
  </servlet-mapping>
</web-app>
